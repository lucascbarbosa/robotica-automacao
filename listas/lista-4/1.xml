<robot name="kinova">
    <!--  URDF viewer www.mymodelrobot.appspot.com  -->
    <!--  ******** Kinematic Properties (Joints) ********  -->
    <joint name="world_joint" type="fixed">
        <parent link="world" />
        <child link="link0" />
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
    </joint>
    <joint name="joint1" type="revolute">
        <origin xyz="0.0 0.0 1.0" rpy="0.0 0.0 0.0" />
        <parent link="link0" />
        <child link="link1" />
        <axis xyz="0 0 1" />
    </joint>
    <joint name="joint2" type="continuous">
        <origin xyz="0.5 0.0 0.0" rpy="0.0 0.0 0.0" />
        <parent link="link1" />
        <child link="link2" />
        <axis xyz="-1 0 0" />
    </joint>
    <joint name="joint3" type="prismatic">
        <origin xyz="0.0 0.0 0.5" rpy="0.0 0.0 0.0" />
        <parent link="link2" />
        <child link="link3" />
        <axis xyz="0 0 1" />
        <limit lower="0.0" upper="0.6"></limit>
    </joint>
    <joint name="joint4" type="continuous">
        <origin xyz="0.5 0.0 0.0" rpy="0.0 0.0 0.0" />
        <parent link="link3" />
        <child link="link4" />
        <axis xyz="-1 0 0" />
    </joint>
    <!--  ******** Visualization Properties (Links) ********  -->
    <material name="blue">
        <color rgba="0 0 .8 1" />
    </material>
    <material name="white">
        <color rgba="1 1 1 1" />
    </material>
    <link name="world" />
    <link name="link0" />
    <link name="link1">
        <visual>
            <origin xyx="0.0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder length="0.5" radius="0.2" />
            </geometry>
            <material name="blue" />
        </visual>
    </link>
    <link name="link2">
        <visual>
            <origin xyx="0 0 0" rpy="0 1.57 0" />
            <geometry>
                <cylinder length="0.6" radius="0.2" />
            </geometry>
            <material name="blue" />
        </visual>
    </link>
    <link name="link3">
        <visual>
            <origin xyx="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder length="0.6" radius="0.2" />
            </geometry>
            <material name="white" />
        </visual>
    </link>
    <link name="link4">
        <visual>
            <origin xyx="0 0 0" rpy="0 1.57 0" />
            <geometry>
                <cylinder length="0.6" radius="0.2" />
            </geometry>
            <material name="blue" />
        </visual>
    </link>
</robot>